<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#edf3f5"
    tools:context="com.example.jogle.attendance.JGAlarmActivity">

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:adjustViewBounds="false"
        android:background="#106bb9" />

    <RelativeLayout
        android:id="@+id/linearLayout4"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <RelativeLayout
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/left_btn"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="6dp"
                android:scaleType="center"
                android:src="@drawable/tt_top_back" />

            <TextView
                android:id="@+id/left_txt"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center_vertical"
                android:layout_toRightOf="@id/left_btn"
                android:gravity="center_vertical"
                android:text="返回"
                android:textColor="@android:color/white"
                android:textSize="14sp" />
        </RelativeLayout>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="考勤提醒"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffffff"
            android:textSize="18sp" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_below="@+id/imageButton4"
        android:orientation="horizontal"></LinearLayout>

    <RelativeLayout
        android:id="@+id/linearLayout15"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout6"
        android:background="#FFFFFF"
        android:orientation="vertical"
        android:paddingBottom="@dimen/cell_padding_bottom"
        android:paddingLeft="@dimen/cell_padding_right"
        android:paddingRight="@dimen/cell_padding_right"
        android:paddingTop="@dimen/cell_padding_top">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:text="考勤提醒"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ff000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/textView8"
            android:text="开启后在设定时间点会收到提醒"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#dedede"
            android:textSize="13sp" />

        <Switch
            android:id="@+id/switch1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:textOff="关"
            android:textOn="开" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/linearLayout15"
        android:orientation="horizontal"></LinearLayout>

    <RelativeLayout
        android:id="@+id/linearLayout16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/linearLayout7"
        android:background="#FFFFFF"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/cell_padding_bottom"
        android:paddingLeft="@dimen/cell_padding_right"
        android:paddingRight="@dimen/cell_padding_right"
        android:paddingTop="@dimen/cell_padding_top">

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:text="上班前"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ff000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:text="上班前"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ff000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:text="5分钟"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#dedede"
            android:textSize="15sp" />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/linearLayout17"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/linearLayout16"
        android:orientation="horizontal" />

    <RelativeLayout
        android:id="@+id/linearLayout18"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/linearLayout17"
        android:background="#FFFFFF"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/cell_padding_bottom"
        android:paddingLeft="@dimen/cell_padding_right"
        android:paddingRight="@dimen/cell_padding_right"
        android:paddingTop="@dimen/cell_padding_top">

        <TextView
            android:id="@+id/textView29"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:text="下班前"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ff000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/textView36"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:text="5分钟"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#dedede"
            android:textSize="15sp" />

    </RelativeLayout>


    <LinearLayout
        android:id="@+id/linearLayout19"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@+id/linearLayout18"
        android:layout_centerHorizontal="true"
        android:orientation="horizontal" />

</RelativeLayout>
